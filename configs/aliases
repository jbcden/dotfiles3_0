# Unix
alias mkdir='mkdir -p'
alias l='ls -CF'
alias ls='ls --color'
alias ll='ls -alF'
alias la='ls -A'
alias e="$EDITOR"
alias v="$VISUAL"

# vim
alias vim="stty stop '' -ixoff ; vim"
alias vi='vim'

# tmux
alias tls='tmux ls'
alias tat='tmux a -t'

# ctags
# alias ctags="`brew --prefix`/bin/ctags"
# Bundler
alias b="bundle"
alias be="bundle exec"

# Include custom aliases
[[ -f ~/.aliases.local ]] && source ~/.aliases.local


alias vim='nvim'
